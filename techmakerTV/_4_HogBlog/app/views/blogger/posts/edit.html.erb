<% provide(:page_title, "Edit #{@post.title}") %>
<% provide(:blogger, 'active') %>
<h1>Editing Post</h1>
<%= render 'form', post: @post, url: blogger_post_url(@post) %>
<%= link_to 'Show', blogger_post_path(@post) %> |
<%= link_to 'Back', blogger_posts_path %>